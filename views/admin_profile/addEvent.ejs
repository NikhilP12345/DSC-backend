<% include ../admin_partials/header %>


 <!-- Content Wrapper. Contains page content -->
 <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Add Event</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Project Add</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <form action="" method="post">
    <form class="content">
      <div class="row">
        <div class="col-md-6">
          <div class="card card-primary">
            <div class="card-header">
              <h3 class="card-title">General</h3>

              <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                  <i class="fas fa-minus"></i></button>
              </div>
            </div>
            <form action="/admin/<%= dict.id%>/add" method="post">
            <div class="card-body">
              <div class="form-group">
                <label for="inputName">Event Name</label>
                <input type="text" id="inputName" class="form-control" name="event_name">
              </div>
              <div class="form-group">
                <label for="inputDescription">About Event</label>
                <textarea id="inputDescription" class="form-control" rows="4" name="about_event"></textarea>
              </div>
              <div class="form-group">
                <label for="inputDescription">About Workshop</label>
                <textarea id="inputDescription" class="form-control" rows="4" name="about_workshop"></textarea>
              </div>
              <div class="form-group">
                <label for="inputDescription">Learning For Student</label>
                <textarea id="inputDescription" class="form-control" rows="4" name="learning_for_student"></textarea>
              </div>
              <div class="form-group">
                <label for="inputDescription">Note For Student</label>
                <textarea id="inputDescription" class="form-control" rows="4" name="note_for_student"></textarea>
              </div>
              <div class="form-group">
                <label for="inputDescription">Registration Process</label>
                <textarea id="inputDescription" class="form-control" rows="4" name="registration_process"></textarea>
              </div>
              <div class="form-group">
                <label for="inputClientCompany">Image Source</label>
                <input type="text" id="inputClientCompany" class="form-control" name="image">
              </div>
              <div class="form-group">
                <label for="inputProjectLeader">Event Small</label>
                <input type="text" id="inputProjectLeader" class="form-control" name = "event_small">
              </div>
              <div class="form-group">
                <label for="inputProjectLeader">MemberTeams</label>
                <input type="number" id="inputProjectLeader" class="form-control" name = "MemberTeams">
              </div>
              <div class="row">
                <div class="col-12">
                  <input type="submit" value="Create new Event" class="btn btn-success float-right">
                </div>
              </div>
              <div>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
        </div>
      </div>
    </form>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->



<% include ../admin_partials/footer %>
                    <source src="wp-content/themes/wfn/circle.ogv" type="video/ogv">
                    <source src="wp-content/themes/wfn/circle.webm" type="video/webm">
                </video>
                <a class="home-main__arrow j-anchor" href="#design">
                    <span class="home-main__arrow-text">Start</span>
                    <svg class="home-main__arrow-icon">
                        <use xlink:href="#icon-arrow-pagination"></use>
                    </svg>
                </a>
                <div class="home-main__title-wrapper">
                    <div class="home-main__tagline-wrapper">
                        <span class="home-main__tagline-line"></span>
                        <!--
                        <h5 class="home-main__tagline">Powered by <span style="color: #4285F4">g</span><span style="color: #EA4335">o</span><span style="color: #FBBC05">o</span><span style="color: #4285F4">g</span><span style="color: #34A853">l</span><span style="color: #EA4335">e</span></h5>-->
                    </div>
